{
  "author": {
    "name": "Ron Dekker",
    "url": "https://rondekker.nl"
  },
  "name": "planckmatch",
  "version": "0.1.0-alpha.0",
  "description": "Small and simple matching library using glob patterns.",
  "license": "ISC",
  "keywords": [
    "extended",
    "glob",
    "patterns",
    "wildcard",
    "expansion",
    "multiple",
    "matches",
    "regexp",
    "regular",
    "expressions"
  ],
  "homepage": "https://github.com/redkenrok/node-planckmatch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redkenrok/node-planckmatch.git"
  },
  "bugs": {
    "url": "https://github.com/redkenrok/node-planckmatch/issues"
  },
  "main": "library/index.js",
  "files": [
    "*.js",
    "library/"
  ],
  "scripts": {
    "benchmark": "node benchmark/dirty.js && node benchmark/optimized.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "pretest": "eslint *.js library/**/*.js test/**/*.js benchmark/**/*.js",
    "test": "nyc --reporter=text ava test/**/*.js --verbose"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^5.3.0",
    "nyc": "^13.0.1"
  }
}
