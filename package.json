{
  "author": {
    "name": "Ron Dekker",
    "url": "https://rondekker.nl"
  },
  "name": "planckmatch",
  "version": "0.0.3",
  "description": "Small and simple matching library using glob patterns.",
  "license": "ISC",
  "keywords": [
    "extended",
    "glob",
    "patterns",
    "multiple",
    "matches",
    "regexp",
    "regular",
    "expressions"
  ],
  "homepage": "https://github.com/redkenrok/node-planckmatch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redkenrok/node-planckmatch.git"
  },
  "bugs": {
    "url": "https://github.com/redkenrok/node-planckmatch/issues"
  },
  "main": "library/index.js",
  "files": [
    "*.js",
    "library/"
  ],
  "scripts": {
    "benchmark": "node benchmark/dirty.js && node benchmark/optimized.js",
    "pretest": "eslint library/**/*.js test/**/*.js benchmark/**/*.js",
    "test": "ava test/**/*.js --verbose",
    "prepublish": "generate-export-aliases"
  },
  "config": {
    "exportAliases": {
      "match": "./library/match.js",
      "parse": "./library/parse.js"
    }
  },
  "dependencies": {
    "globrex": "^0.1.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^5.3.0",
    "generate-export-aliases": "^1.1.0"
  }
}
